--open area
local i = game:GetService("Workspace").GMDoors

i.Door:Destroy()
i.KeycardReader:Destroy()
i.Closed:Destroy()
i.Open_1:Destroy()
i.Keycard:Destroy()
i.Keycard_Pos:Destroy()
i.Frame:Destroy()
i.Door_Script:Destroy()

game:GetService("Workspace").SA:Destroy()

-- Gui to Lua
-- Version: 3.2

-- Instances:

local MainGui = Instance.new("ScreenGui")

--Properties:

MainGui.Name = "MainGui"
MainGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
MainGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

-- Scripts:

local function QKHGI_fake_script() -- MainGui.DoorOpen1 
	local script = Instance.new('LocalScript', MainGui)

	local sound = Instance.new("Sound")
	
	sound.Name = "Sound"
	sound.SoundId = "http://www.roblox.com/asset/?id=157167203" --Song
	sound.Volume = 1 
	sound.PlaybackSpeed = 1 
	sound.Looped = true
	sound.Archivable = false
	
	sound.Parent = game.Workspace
	
	wait(0)
	
	sound:Play()
end
coroutine.wrap(QKHGI_fake_script)()
