--prep positon 1
local CreateInstance = Instance.new("Part", game.Workspace)
CreateInstance.Anchored = true
CreateInstance.Size = Vector3.new(10,0.1,10)
CreateInstance.Position = Vector3.new(-53.3925171, 3.01800799, -83.6023788, 0.999990582, -3.68257119e-10, 0.00434534252, 3.22653015e-10, 1, 1.04956417e-08, -0.00434534252, -1.04941407e-08, 0.999990582)
CreateInstance.CanCollide = true
CreateInstance.Archivable = false
CreateInstance.Transparency = 1
CreateInstance.Name = "Stande3r"

game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = true
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 0

local TweenService = game:GetService("TweenService")
local target = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart

local Info = TweenInfo.new(
	1,
	Enum.EasingStyle.Linear,
	Enum.EasingDirection.InOut,
	0,
	false,
	0
)

local Fire = 
{
	CFrame = game.Workspace.Stande3r.CFrame
}

local RunServx = TweenService:Create(target, Info, Fire)

RunServx:Play()
wait(5)
CreateInstance:Destroy()


--bypass AC [just incase]
local CreateInstance = Instance.new("Part", game.Workspace)
CreateInstance.Anchored = true
CreateInstance.Size = Vector3.new(1,0.1,1)
CreateInstance.Position = Vector3.new(-20000.6379128, -100.01800799, -20000.4985504)
CreateInstance.CanCollide = false
CreateInstance.Archivable = false
CreateInstance.Transparency = 1
CreateInstance.Name = "Stande3r"

game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = true
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 0

local TweenService = game:GetService("TweenService")
local target = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart

local Info = TweenInfo.new(
	1,
	Enum.EasingStyle.Linear,
	Enum.EasingDirection.InOut,
	0,
	false,
	0
)

