OpenUI.BackgroundColor3 = Color3.fromRGB(75, 75, 75)
OpenUI.BorderSizePixel = 4
OpenUI.Position = UDim2.new(0.0114503149, 0, 0.522015214, 0)
OpenUI.Size = UDim2.new(0.071941115, 0, 0.0508747883, 0)
OpenUI.Visible = false
OpenUI.Text = "Open"
OpenUI.TextColor3 = Color3.fromRGB(190, 190, 190)
OpenUI.TextSize = 22.000

-- Scripts:

local function WSNTJA_fake_script() -- God_Mode.LocalScript 
	local script = Instance.new('LocalScript', God_Mode)

	script.Parent.MouseButton1Down:Connect(function()
		for i,v in pairs(game.workspace:GetChildren()) do
			if v.Name == "Part" then
				v:Destroy()
			end
		end
	end)
end
coroutine.wrap(WSNTJA_fake_script)()
local function TCBGF_fake_script() -- Kill_Bot.LocalScript 
	local script = Instance.new('LocalScript', Kill_Bot)

	_G.Run = true
	script.Parent.MouseButton1Down:Connect(function()
		_G.Run = true
		while _G.Run == true do wait()
		for i,v in pairs(game.Workspace:GetChildren()) do
			if v.Name == script.Parent.Parent.Bot_Kill_Value.Text then
				if game.Players.LocalPlayer.Character.Humanoid.Health <= 20 then
					warn("Abort Kill Switch")
				else
					game.Players.LocalPlayer.Character:WaitForChild("Sword_0").Handle.Position = v:WaitForChild("HumanoidRootPart", 1000).Position
					game.Players.LocalPlayer.Character.Sword_0:Activate()
				end
			end
			end
		end
	end)
	
	script.Parent.MouseButton2Down:Connect(function()
		_G.Run = false
	end)
	
	script.Parent.Parent.Sword_Reach.MouseButton1Down:Connect(function()
		_G.Run = false
	end)
end
coroutine.wrap(TCBGF_fake_script)()
local function WYOS_fake_script() -- Sword_Reach.LocalScript 
	local script = Instance.new('LocalScript', Sword_Reach)

	_G.Run = true
	script.Parent.MouseButton1Down:Connect(function()
		_G.Run = true
		-- script
		local P = game.Players
		local LP = game.Players.LocalPlayer
		local LPC = game.Players.LocalPlayer.Character
		local Tool = LP.Backpack.Sword_0
	
		for i,v in pairs(LP.Backpack:GetChildren()) do
			if v.Name == "Sword_0" then
				LPC.Humanoid:EquipTool(Tool)
				print("Equipping tool")
				v.Handle.Mesh:Destroy()
				v.Handle.Size = Vector3.new(0.1, 0.1, 350)
			else
				for i,v in pairs(LP.Character:GetChildren()) do
					if v.Name == "Sword" then 
						LPC.Sword.Handle.Mesh:Destroy()
						LPC.Sword.Handle.Size = Vector3.new(0.1, 0.1, 350)
					end
				end
			end
	
			while _G.Run == true do wait()
				for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
					if v.Name == "Humanoid" then
						if v.Health == 0 then
							warn("Stopping All Functions")
							warn("Waiting For Humanoid Death...")
							warn("Player Has Died")
							print("Reloading Assets...")
							wait(4)
							for x,q in pairs(LP.Backpack:GetChildren()) do
								if q.Name == "Sword_0" then
									game.Players.LocalPlayer.Character.Humanoid:EquipTool(q)
									game.Players.LocalPlayer.Character.Sword_0.Handle.Mesh:Destroy()
									game.Players.LocalPlayer.Character.Sword_0.Handle.Size = Vector3.new(0.1, 0.1, 350)
								else
									print("Sword Already Equipped")
									game.Players.LocalPlayer.Character.Sword_0.Handle.Mesh:Destroy()
									game.Players.LocalPlayer.Character.Sword_0.Handle.Size = Vector3.new(0.1, 0.1, 350)
								end
							end
						end
					end
				end
			end
		end
	end)
	
	script.Parent.MouseButton2Down:Connect(function()
		_G.Run = false
	end)
	
	script.Parent.Parent.Sword_Auto_Equip.MouseButton1Down:Connect(function()
		_G.Run = false
	end)
	
	script.Parent.Parent.Kill_Bot.MouseButton1Down:Connect(function()
		_G.Run = false
	end)
end
coroutine.wrap(WYOS_fake_script)()
local function XGMA_fake_script() -- WalkSpeed.LocalScript 
	local script = Instance.new('LocalScript', WalkSpeed)

	script.Parent.MouseButton1Down:Connect(function()
		local U = game.Players.LocalPlayer.Character
		U.Humanoid.WalkSpeed = script.Parent.Parent.WalkSpeed_Value.Text
	end)
end
