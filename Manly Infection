local function NTSHZ_fake_script() -- Close.LocalScript 
	local script = Instance.new('LocalScript', Close)

	script.Parent.MouseButton1Down:Connect(function()
		script.Parent.Parent.Visible = false
		script.Parent.Parent.Parent.OpenUI.Visible = true
	end)
end
coroutine.wrap(NTSHZ_fake_script)()
local function IHHCLYE_fake_script() -- UI.Draggable 
	local script = Instance.new('LocalScript', UI)

	local UserInputService = game:GetService("UserInputService")
	
	local gui = script.Parent
	
	local dragging
	local dragInput
	local dragStart
	local startPos
	
	local function update(input)
		local delta = input.Position - dragStart
		gui.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
	end
	
	gui.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = true
			dragStart = input.Position
			startPos = gui.Position
	
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)
	
	gui.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput = input
		end
	end)
	
	UserInputService.InputChanged:Connect(function(input)
		if input == dragInput and dragging then
			update(input)
		end
	end)
end
coroutine.wrap(IHHCLYE_fake_script)()
local function QVCRJA_fake_script() -- UI.AntiCheatBypass 
	local script = Instance.new('LocalScript', UI)

	while true do wait()
		for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
			if v.Name == "Humanoid" then
				if v.Health == 0 then
					warn("Player Has Died")
					print("Attempting to Repair Script Modules...")
					wait(6)
					for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
						if v.Name == "PlayerHealthbarGui" then
							v.Disabled = true
						end
						if v.Name == "SkillRatingScript" then
							v.Disabled = true
						end
						if v.Name == "StreaksScript" then
							v.Disabled = true
						end
						if v.Name == "SwordConnectingScript" then
							v.Disabled = true
						end
					end
				else
					for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
						if v.Name == "PlayerHealthbarGui" then
							v.Disabled = true
						end
						if v.Name == "SkillRatingScript" then
							v.Disabled = true
						end
						if v.Name == "StreaksScript" then
							v.Disabled = true
						end
						if v.Name == "SwordConnectingScript" then
							v.Disabled = true
						end
					end
				end
			end
		end
	end
	
	
	
end
coroutine.wrap(QVCRJA_fake_script)()
local function CSTR_fake_script() -- UI.Destroy_Kill_Bricks 
	local script = Instance.new('LocalScript', UI)

	for i,v in pairs(game.Workspace:GetChildren()) do
		if v.Name == "Part" then
			v:Destroy()
		end
		if v.Name == "Kill brick" then
			v:Destroy()
		end
	end
	
end
coroutine.wrap(CSTR_fake_script)()
local function DWXUTYD_fake_script() -- OpenUI.LocalScript 
	local script = Instance.new('LocalScript', OpenUI)

	script.Parent.MouseButton1Down:Connect(function()
		script.Parent.Parent.UI.Visible = true
		script.Parent.Visible = false
	end)
end
coroutine.wrap(DWXUTYD_fake_script)()
