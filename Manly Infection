local function SZHZIA_fake_script() -- Tp_To_Player.LocalScript 
	local script = Instance.new('LocalScript', Tp_To_Player)

	script.Parent.MouseButton1Down:Connect(function()
		local Player = game.Players.LocalPlayer
		
		for i,v in pairs(game.Players:GetChildren()) do
			if v.Name == script.Parent.Parent.Tp_To_Player_Value.Text then
				game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.Character.HumanoidRootPart.CFrame
			end
		end
	end)
end
coroutine.wrap(SZHZIA_fake_script)()
local function RQTNBC_fake_script() -- Tween_To_Plr.LocalScript 
	local script = Instance.new('LocalScript', Tween_To_Plr)

	script.Parent.MouseButton1Down:Connect(function()
		local TweenService = game:GetService("TweenService")
		local target = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart
	
		local Info = TweenInfo.new(
			script.Parent.Parent.Tween_Speed_Value.Text,
			Enum.EasingStyle.Linear,
			Enum.EasingDirection.InOut,
			0,
			false,
			1
		)
	
		local Fire = 
			{
				CFrame = script.Parent.Parent.Tween_To_Plr.Text
			}
	
		local RunServx = TweenService:Create(target, Info, Fire)
	
		RunServx:Play()
	end)
end
coroutine.wrap(RQTNBC_fake_script)()
local function XXDSDTG_fake_script() -- Loop_Kill_Plr.LocalScript 
	local script = Instance.new('LocalScript', Loop_Kill_Plr)

	_G.Run = true
	script.Parent.MouseButton1Down:Connect(function()
		_G.Run = true
		for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
			if v.Name == "Bat" then
				game.Players.LocalPlayer.Backpack.Bat.Handle.Mesh:Destroy()
				game.Players.LocalPlayer.Backpack.Bat.Handle.Size = Vector3.new(1,1,3)
				game.Players.LocalPlayer.Character.Humanoid:EquipTool(v)
			else
				warn("Tool Already Equipped or is missing")
				game.Players.LocalPlayer.Character.Bat.Handle.Mesh:Destroy()
				game.Players.LocalPlayer.Character.Bat.Handle.Size = Vector3.new(1, 1, 3)
			end
		end
		for i,v in pairs(game.Players:GetChildren()) do
			if v.Name == script.Parent.Parent.Loop_Kill_Plr.Text then
				print("Searching")
				game.Players.LocalPlayer.Character.Bat.Handle.CFrame = v.Character.HumanoidRootPart.CFrame
			else
				warn("Player Searcher V1 Failed to find player!")
			end
		end
	
		while _G.Run == true do wait()
			for i,v in pairs(game.Players.LocalPlayer:GetChildren()) do
				if v.Name == "Humanoid" then
					if v.Health == 0 then
						warn("Player Died")
						print("Attempting to sustain Errors")
						wait(6)
						for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
							if v.Name == "Bat" then
								game.Players.LocalPlayer.Backpack.Bat.Handle.Mesh:Destroy()
								game.Players.LocalPlayer.Backpack.Bat.Handle = Vector3.new(1,1,3)
								game.Players.LocalPlayer.Character.Humanoid:EquipTool(v)
							else
								warn("Tool Already Equipped or is missing")
								game.Players.LocalPlayer.Character.Bat.Handle.Mesh:Destroy()
								game.Players.LocalPlayer.Character.Bat.Handle.Size = Vector3.new(1, 1, 3)
							end
						end
						for i,v in pairs(game.Players:GetChildren()) do
							if v.Name == script.Parent.Parent.Loop_Kill_Plr.Text then
								game.Players.LocalPlayer.Character.Bat.Handle.Position = v.Character.HumanoidRootPart.Position
							end
						end
					end
				end
			end
		end
	end)
	
	script.Parent.MouseButton2Down:Connect(function()
		_G.Run = false
	end)
end
coroutine.wrap(XXDSDTG_fake_script)()
local function JXSIIOR_fake_script() -- BypassAC.LocalScript 
	local script = Instance.new('LocalScript', BypassAC)

	_G.Run = true
	script.Parent.MouseButton1Down:Connect(function()
		while _G.Run == true do wait()
		for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
			if v.Name == "Humanoid" then
				if v.Health == 0 then
					warn("Player has Died")
					print("Resetting A-C Bypass...")
					wait(5.5)
					for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
						if v.Name == "Input" then
							v.Disabled = true
						end
						if v.Name == "input" then
							v:Destroy()
						end
					end
				end
			end
			end
		end
	end)
end
coroutine.wrap(JXSIIOR_fake_script)()
local function OJUSAIA_fake_script() -- UI.Draggable 
	local script = Instance.new('LocalScript', UI)

	local UserInputService = game:GetService("UserInputService")
	
	local gui = script.Parent
	
	local dragging
	local dragInput
	local dragStart
	local startPos
	
	local function update(input)
		local delta = input.Position - dragStart
		gui.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
	end
	
	gui.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = true
			dragStart = input.Position
			startPos = gui.Position
	
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)
	
	gui.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput = input
		end
	end)
	
	UserInputService.InputChanged:Connect(function(input)
		if input == dragInput and dragging then
			update(input)
		end
	end)
end
coroutine.wrap(OJUSAIA_fake_script)()
