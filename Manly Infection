-- Scripts:

local function KXHXV_fake_script() -- AntiGravityOn.LocalScript 
	local script = Instance.new('LocalScript', AntiGravityOn)

	script.Parent.MouseButton1Down:Connect(function()
		script.Parent.Parent.AntiGravityOnDetector.Text = "Anti Gravity Gun = On"
		game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = true
	end)
end
coroutine.wrap(KXHXV_fake_script)()
local function TJWZKKH_fake_script() -- AntiGravityOff.LocalScript 
	local script = Instance.new('LocalScript', AntiGravityOff)

	script.Parent.MouseButton1Down:Connect(function()
		script.Parent.Parent.AntiGravityOnDetector.Text = "Anti Gravity Gun = Off"
		game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = false
	end)
end
coroutine.wrap(TJWZKKH_fake_script)()
local function VJQH_fake_script() -- AntiGravityOnDetector.LocalScript 
	local script = Instance.new('LocalScript', AntiGravityOnDetector)

	local CCf =  true
	
	if CCf == true then
		while true do wait()
		wait(120)
		script.Parent.Text = "Reloading..."
		wait(0.2)
		script.Parent.Text = "..."
		wait(0.3)
		script.Parent.Text = "Reloaded!"
		wait(0.2)
		script.Parent.Text = "Anti Gravity Gun = Off"
			wait(0.3)
		end
	end
end
coroutine.wrap(VJQH_fake_script)()
local function UWJF_fake_script() -- GodeModeOnDetector.LocalScript 
	local script = Instance.new('LocalScript', GodeModeOnDetector)

	local Fire2 = true
	
	if Fire2 == true then
			script.Parent.Parent.GodModeOn.MouseButton1Down:Connect(function()
			wait(0.5)
			script.Parent.Text = "Checking if Synapse X..."
			wait(0.2)
			script.Parent.Text = "Bypassing AntiCheat..."
			wait(0.5)
			script.Parent.Text = "Loading..."
			wait(0.2)
			script.Parent.Text = "Enabling God Mode..."
			wait(0.1)
			script.Parent.Text = "Gode Mode = On"
		end)
	end
end
coroutine.wrap(UWJF_fake_script)()
