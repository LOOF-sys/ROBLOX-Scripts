-- Scripts:

local function MKGZYX_fake_script() -- OpenUI.LocalScript 
	local script = Instance.new('LocalScript', OpenUI)

	script.Parent.MouseButton1Down:Connect(function()
		script.Parent.Visible = false
		script.Parent.Parent.UI.Visible = true
	end)
end
coroutine.wrap(MKGZYX_fake_script)()
local function FSAFXG_fake_script() -- MainGui.GodModeChecker 
	local script = Instance.new('LocalScript', MainGui)

	local run = true
	
	while run == true do wait(.5)
		if game.Players.LocalPlayer.Character.Humanoid.Health == 0 then
			script.Parent.UI.GodeModeOnDetector.Text = "Gode Mode = Off"
		end
	end
end
coroutine.wrap(FSAFXG_fake_script)()
