-- manly infection

local ui = loadstring(game:HttpGet("https://raw.githubusercontent.com/wally-rblx/uwuware-ui/main/main.lua",true))()

local window = ui:CreateWindow("Main")

window:AddButton({text = "Bypass Anti Cheat",callback = function ()
local MT = getrawmetatable(game)
local odlNamecall = MT.__namecall
setreadonly(MT,false)

MT.__namecall = newcclosure(function(self, ...)
	local method = getnamecallmethod()
	local args = {...}

	if method:lower() == "fireserver" and self.Name == "event" then
		return "fuck you bitch"--odlNamecall(self,1000)
	end

	return odlNamecall(self, ...)
end)

	setreadonly(MT,true)
	warn("Bypassed A-C")
end})

window:AddButton({text = "God Mode",callback = function ()
    game.Players.LocalPlayer.Character.input:Destroy()
	game.Players.LocalPlayer.Character.Input.Disabled = true

	local PTX = Instance.new("Part", game.Workspace)
	PTX.Anchored = true
	PTX.CanCollide = false
	PTX.Transparency = 0.5
	PTX.Position = Vector3.new(-17.588131, 38.2899971, -295.128479, 0.219850376, 6.10635809e-08, 0.975533605, -7.23261593e-08, 1, -4.62953267e-08, -0.975533605, -6.03785537e-08, 0.219850376)
	PTX.Size = Vector3.new(1,1,1)
	PTX.Name = "PartRX"

	-- tween
	local TweenService = game:GetService("TweenService")
	local target = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart

	local Info = TweenInfo.new(
		0.01,
		Enum.EasingStyle.Linear,
		Enum.EasingDirection.InOut,
		0,
		false,
		1
	)

	local Fire = 
		{
			CFrame = game.Workspace.PartRX.CFrame
		}

	local RunServx = TweenService:Create(target, Info, Fire)

	RunServx:Play()
	PTX:Destroy()
	game.Players.LocalPlayer.Character.Input.Disabled = true
	game.Players.LocalPlayer.Character:WaitForChild("ManScript"):Destroy()
	game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 30
	game.Players.LocalPlayer.Character.Humanoid.JumpPower = 50
end})

window:AddButton({text = "Mod Bat",callback = function ()
    for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
		print(v)
		if v.Name == "Bat" then
			v.Handle.Massless = true
			v.Handle.Mesh:Destroy()
			v.Handle.Size = Vector3.new(5,5,50)
			print("Modded")
		else
			warn("Players Bat Not In Inventory")
			warn("Checking If Equipped...")
			for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
				print(v)
				if v.Name == "Bat" then
					v.Handle.Massless = true
					v.Handle.Mesh:Destroy()
					v.Handle.Size = Vector3.new(3,3,20)
					print("Modded")
				else
					warn("Bat Could Not Be Found || REPORT THIS ISSUE TO THE DISCORD")
				end
			end
		end
	end
end})

window:AddSlider({text = "WalkSpeed",min = 16,max = 2000,dual = true,callback = function (value)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = value
end})

window:AddSlider({text = "JumpPower",min = 50,max = 200,float = 3,callback = function (value)
    game.Players.LocalPlayer.Character.Humanoid.HipHeight = value
end})

window:AddSlider({text = "Gravity",min = 0,max = 192,dual = true,callback = function (value)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = value
end})

window:AddBox({text = "Tp To player",value = "Player Name",callback = function (text,confirmed)
    for i,v in pairs(game.Players:GetChildren()) do
        if v.Name == text then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.Character.HumanoidRootPart.CFrame
            print("Tped")
        end
    end
end})

window:AddBox({text = "Loop Kill Infected",value = "Player Name",callback = function (text,confirmed)
for x,k in pairs(game.Players:GetChildren()) do
    print(k)
    if k.Name == text then
    while wait() do
    for i,v in pairs(k.Character:GetChildren()) do
        if v.Name == "HumanoidRootPart" then
            for i,v in pairs(game.Players.LocalPlayer.Character:WaitForChild("Bat", 1000).Handle:GetChildren()) do
                if v.Name == "Mesh" then
                    v:Destroy()
                end
            end
            game.Players.LocalPlayer.Character.Bat.Handle.Position = v.Position
            game.Players.LocalPlayer.Character.Bat.input:FireServer()
        end
    end
end
end
end
end})

window:AddToggle({text = "Anti Gravity Gun",flag = "AntiGravChecked",callback = function (checked)
while wait() do
    if ui.flags.AntiGravChecked then
        game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = true
    else
        game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = false
    end
end
end})

window:AddBind({text = "Close/Open UI",key = Enum.KeyCode.P,hold = false,callback = function ()
    ui:Close()
end})

ui:Init()
