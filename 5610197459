-- window 3

window3:AddToggle({text = "Auto Pistol Grabber",flag = "PistolGrabChecked",callback = function (checked)
    while wait() do
        if ui.flags.PistolGrabChecked then
            for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
                if v.Name == "Humanoid" then
                    if v.Health == 0 then
                        warn("Player Has Died... Resseting AUto GRabbers")
                    else
            local pistol = game:GetService("Workspace").Map.GunSpawner:WaitForChild("GunCollector").Handle
            if game.Players.LocalPlayer.Character.Humanoid.Health == 0 then
                warn("Player Has Died... Resetting Auto GRab")
                wait(WEAPON_GRAB_SPEED_VALUE)
            else
            firetouchinterest(game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart"),game.Workspace.Map:WaitForChild("GunSpawner"):WaitForChild("GunCollector").Handle,1) 
            firetouchinterest(game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart"),game.Workspace.Map:WaitForChild("GunSpawner"):WaitForChild("GunCollector").Handle,0)        
            end
        end
        end
        end
        end
    end
end})

window3:AddToggle({text = "Auto Shotgun Grabber",flag = "ShotgunGrabChecked",callback = function (checked)
    while wait() do
        if ui.flags.ShotgunGrabChecked then
            for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
                if v.Name == "Humanoid" then
                    if v.Health == 0 then
                        warn("Player Has Died... Resseting AUto GRabbers")
                    else
            local shotgun = game:GetService("Workspace").Map.ShotgunSpawner:WaitForChild("ShotgunCollector").Handle
            if game.Players.LocalPlayer.Character.Humanoid.Health == 0 then
                    warn("Player Has Died... Resetting Auto GRab")
                    wait(WEAPON_GRAB_SPEED_VALUE)
                else
            firetouchinterest(game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart"),game.Workspace.Map:WaitForChild("ShotgunSpawner"):WaitForChild("ShotgunCollector").Handle,1) 
            firetouchinterest(game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart"),game.Workspace.Map:WaitForChild("ShotgunSpawner"):WaitForChild("ShotgunCollector").Handle,0)
                end
            end
            end
        end
        end
    end
end})

window3:AddToggle({text = "Auto Knife Grabber",flag = "KnifeGrabChecked",callback = function (checked)
    while wait() do
        if ui.flags.KnifeGrabChecked then
            for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
                if v.Name == "Humanoid" then
                    if v.Health == 0 then
                        warn("Player Has Died... Resseting AUto GRabbers")
                    else
            local knife = game:GetService("Workspace").Map.KnifeHouse.KnifeSpawner:WaitForChild("KnifeCollector").Handle
            if game.Players.LocalPlayer.Character:WaitForChild("Humanoid").Health == 0 then
                warn("Player Has Died... Resetting Auto GRab")
                wait(WEAPON_GRAB_SPEED_VALUE)
            else
            firetouchinterest(game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart"),game.Workspace.Map:WaitForChild("KnifeSpawner"):WaitForChild("KnifeCollector").Handle,1) 
            firetouchinterest(game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart"),game.Workspace.Map:WaitForChild("KnifeSpawner"):WaitForChild("KnifeCollector").Handle,0)
            end
        end
        end
    end
    end
end
end})

window3:AddBox({text = "Kill Player [Pistol Required]",value = "Players Name Here",callback = function (text,confirmed)
    for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
        if v.Name == "Gun" then
            game.Players.LocalPlayer.Character.Humanoid:EquipTool(v)
            wait(0.1)
            for i,v in pairs(game.Players:GetChildren()) do
                print(v)
                if v.Name == text then
                    for i,v in pairs(v.Character:GetChildren()) do
                        print(v)
                        if v.Name == "HumanoidRootPart" then
                            local ohString1 = "Fire"
                            game.Players.LocalPlayer.Character.Gun.DamageEvent:FireServer(ohString1, v)
                            print("Killed Target")
                        else
                            warn("Could Not Kill Target")
                        end
                    end
               end
            end
        else
            warn("Gun already equipped or is non existent")
            for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
                if v.Name == "Gun" then
                    for i,v in pairs(game.Players:GetChildren()) do
                        print(v)
                        if v.Name == text then
                            for i,v in pairs(v.Character:GetChildren()) do
                                print(v)
                                if v.Name == "HumanoidRootPart" then
                                    local ohString1 = "Fire"
                                    game.Players.LocalPlayer.Character.Gun.DamageEvent:FireServer(ohString1, v)
                                    print("Killed Target")
                                else
                                    warn("Could Not Kill Target")
                                end
                            end
                        end
                    end
                else
                    warn("Your supposted to get a gun befre you use this")
                end
            end
        end
    end
end})

-- window 4
window4:AddSlider({text = "Ragdoll Reset Delay",min = 5.5,max = 10,dual = true,callback = function (value)
    ANTIRAGDOLL_SPEED_RATE = value
end})

window4:AddSlider({text = "Farm Reset Delay",min = 5.5,max = 10,dual = true,callback = function (value)
    RESET_FARM_AND_RAGDOLL_RATE = value
end})

window4:AddSlider({text = "Weapons Reset Delay",min = 5.5,max = 10,dual = true,callback = function (value)
    WEAPON_GRAB_SPEED_RATE = value
end})

ui:Init()
