-- gang up on people simulator

local ui = loadstring(game:HttpGet("https://raw.githubusercontent.com/wally-rblx/uwuware-ui/main/main.lua",true))()

local window = ui:CreateWindow("Main")
local window2 = ui:CreateWindow("Teleports")
local window3 = ui:CreateWindow("Fun Stuff")
local window4 = ui:CreateWindow("Misc")

-- rate = delay

FARM_SPEED_VALUE = 0
ANTIRAGDOLL_SPEED_RATE = 5.5
RESET_FARM_AND_RAGDOLL_RATE = 5.5
WEAPON_GRAB_SPEED_RATE = 5.5
-- window 1 stuff
window:AddToggle({text = "Auto Farm",flag = "AutoFarmChecked",callback = function (checked)
while wait(FARM_SPEED_VALUE) do
    if ui.flags.AutoFarmChecked then
    for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
        if v.Name == "Humanoid" then
            v.Sit = false
            if v.Health == 0 then
                wait(1)
                warn("Player Has Died")
                warn("Resetting Auto Farm")
                wait(RESET_FARM_AND_RAGDOLL_RATE)
            else
            for i,v in pairs(game.Workspace.Particles:GetChildren()) do
			    if v.Name == "Money" then
				    wait(0.1)
				if game.Players.LocalPlayer.Character.Humanoid.Health == 0 then
                    warn("Player Has Died")
                    wait(RESET_FARM_AND_RAGDOLL_RATE)
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.Bundle.CFrame
                else
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.Bundle.CFrame
                end
			    end
            end
	        for i,v in pairs(game.Workspace:GetChildren()) do
	            if v.Name == "Money" then
	                wait(0.1)
				    if game.Players.LocalPlayer.Character.Humanoid.Health == 0 then
                    warn("Player Has Died")
                    wait(RESET_FARM_AND_RAGDOLL_RATE)
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.Bundle.CFrame
                else
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.Bundle.CFrame
				end
            end
	        end
	        end
        else
        for i,v in pairs(game.Workspace.Particles:GetChildren()) do
			if v.Name == "Money" then
				wait(0.1)
				if game.Players.LocalPlayer.Character.Humanoid.Health == 0 then
                    warn("Player Has Died")
                    wait(RESET_FARM_AND_RAGDOLL_RATE)
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.Bundle.CFrame
                else
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.Bundle.CFrame
                end
			end
        end
	    for i,v in pairs(game.Workspace:GetChildren()) do
	        if v.Name == "Money" then
	            wait(0.1)
	            if game.Players.LocalPlayer.Character.Humanoid.Health == 0 then
                    warn("Player Has Died")
                    wait(RESET_FARM_AND_RAGDOLL_RATE)
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.Bundle.CFrame
                else
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.Bundle.CFrame
                end
	        end
	    end
        end
    end
    end
end
end})

window:AddToggle({text = "Anti Ragdoll",flag = "AntiRagdollChecked",callback = function (checked)
    while wait(ANTIRAGDOLL_SPEED_VALUE) do
        if ui.flags.AntiRagdollChecked then
            for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
                if v.ClassName == "Humanoid" then
                    v.Sit = false
                    print(v)
                if v.Name == "Humanoid" then
                    if v.Health == 0 then
                        warn("Player Has Died")
                        warn("Resetting Anti Ragdoll...")
                        wait(RESET_FARM_AND_RAGDOLL_RATE)
                        for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
		                    if v.Name == "Ragdoll" then
			                    v.RagdollInput.Disabled = true
			                    for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
			                        if v.Name == "Ragdoll" then
                                        v:Destroy()
                                    else
                                        warn("Already Destroyed Instance")
                                    end
			                    end
		                    end
	                    end
                    else
                        for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
		                    if v.Name == "Ragdoll" then
			                    v.RagdollInput.Disabled = true
                                for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
			                        if v.Name == "Ragdoll" then
                                        v:Destroy()
                                    else
                                        warn("Already Destroyed Instance")
                                    end
			                    end
		                    end
                        end
                    end
                    end
                end
            end
        end
    end
end})

window:AddToggle({text = "Auto Deposit",flag = "AutoDepositChecked",callback = function (checked)
    while wait() do
        if ui.flags.AutoDepositChecked then
            local ohTable1 = {
                ["Amount"] = 10,
                ["Mode"] = "D"
            }
        
            game:GetService("ReplicatedStorage").RemoteEvent.ATM:FireServer(ohTable1)
        end
    end
end})

window:AddButton({text = "Give Pistol",callback = function ()
	local pistol = game:GetService("Workspace").Map.GunSpawner:WaitForChild("GunCollector").Handle
    firetouchinterest(game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart"),game.Workspace.Map:WaitForChild("GunSpawner"):WaitForChild("GunCollector").Handle,0)
    firetouchinterest(game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart"),game.Workspace.Map:WaitForChild("GunSpawner"):WaitForChild("GunCollector").Handle,0)
end})

window:AddButton({text = "Give Shotgun",callback = function ()
    local shotgun = game:GetService("Workspace").Map.ShotgunSpawner:WaitForChild("ShotgunCollector").Handle
    firetouchinterest(game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart"),game.Workspace.Map:WaitForChild("ShotgunSpawner"):WaitForChild("ShotgunCollector").Handle,1) 
    firetouchinterest(game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart"),game.Workspace.Map:WaitForChild("ShotgunSpawner"):WaitForChild("ShotgunCollector").Handle,0)
end})

window:AddButton({text = "Give Knife",callback = function ()
	local knife = game:GetService("Workspace").Map.KnifeHouse.KnifeSpawner:WaitForChild("KnifeCollector").Handle
    firetouchinterest(game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart"),game.Workspace.Map:WaitForChild("KnifeSpawner"):WaitForChild("KnifeCollector").Handle,1) 
    firetouchinterest(game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart"),game.Workspace.Map:WaitForChild("KnifeSpawner"):WaitForChild("KnifeCollector").Handle,0)
end})

window:AddSlider({text = "WalkSpeed",min = 16,max = 300,dual = true,callback = function (value)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = value
end})

window:AddSlider({text = "JumpPower",min = 50,max = 300,float = 3,callback = function (value)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = value
end})

window:AddSlider({text = "Gravity",min = 0,max = 192,float = 3,callback = function (value)
    game.Workspace.Gravity = value
end})

window:AddSlider({text = "Auto Farm Delay",min = 0,max = 5,dual = true,callback = function (value)
    FARM_SPEED_VALUE = value
end})

window:AddBox({text = "Teleport To Player",value = "Players Name Here",callback = function (text,confirmed)
    for i,v in pairs(game.Players:GetChildren()) do
        if v.Name == text then
            local timeRemaining = 15

            while timeRemaining > 0 do
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.Character.HumanoidRootPart.CFrame
                wait(0.05)
                timeRemaining = timeRemaining - 1	
            end
        end
    end
end})

window:AddButton({text = "Infinite Jump",callback = function ()
    game:GetService("UserInputService").JumpRequest:Connect(function()
		game:GetService("Players").LocalPlayer.Character:FindFirstChildOfClass("Humanoid"):ChangeState("Jumping")		
	end)
end})

window:AddToggle({text = "Noclip",flag = "NoclipChecked",callback = function (checked)
    while wait() do
        if ui.flags.NoclipChecked then
            game.Players.LocalPlayer.Character.Humanoid:ChangeState(11)
            game.Players.LocalPlayer.Character.Humanoid:ChangeState(11)
            game.Players.LocalPlayer.Character.Humanoid:ChangeState(11)
            game.Players.LocalPlayer.Character.Humanoid:ChangeState(11)
            game.Players.LocalPlayer.Character.Humanoid:ChangeState(11)
            game.Players.LocalPlayer.Character.Humanoid:ChangeState(11)
            game.Players.LocalPlayer.Character.Humanoid:ChangeState(11)
            game.Players.LocalPlayer.Character.Humanoid:ChangeState(11)
            game.Players.LocalPlayer.Character.Humanoid:ChangeState(11)
            game.Players.LocalPlayer.Character.Humanoid:ChangeState(11)
            game.Players.LocalPlayer.Character.Humanoid:ChangeState(11)
            game.Players.LocalPlayer.Character.Humanoid:ChangeState(11)
            game.Players.LocalPlayer.Character.Humanoid:ChangeState(11)
            game.Players.LocalPlayer.Character.Humanoid:ChangeState(11)
        end
    end
end})

window:AddBind({text = "Close/Open UI",key = Enum.KeyCode.P,hold = false,callback = function ()
    ui:Close()
end})

