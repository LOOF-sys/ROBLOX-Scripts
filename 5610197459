	while timeRemaining > 0 do
		game.Players.LocalPlayer.Character:MoveTo(Vector3.new(-71.0635681, 432.722687, 46.3323708, 0.770631909, 3.22215499e-08, 0.637280524, 7.05204695e-09, 1, -5.9088709e-08, -0.637280524, 5.00297759e-08, 0.770631909))
		wait(0.05)
		timeRemaining = timeRemaining - 1	
	end
end})

